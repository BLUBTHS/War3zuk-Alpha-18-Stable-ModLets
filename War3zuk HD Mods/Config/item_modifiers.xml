<configs>
	
<append xpath="/item_modifiers" >
	
	<item_modifier name="HDColtM1911Silencer" installable_tags="M1911" modifier_tags="barrelAttachment" type="attachment">
		<property name="CustomIcon" value="HD Colt M1911 Silencer"/>
		<property name="DescriptionKey" value="HDColtM1911SilencerDesc"/>
		<property name="Stacknumber" value="1"/>
		<property name="DropMeshfile" value="#@modfolder:Resources/HDColtM1911Silencer.unity3d?HDColtM1911Silencer.Prefab"/>
		
		<item_property_overrides name="HD Colt M1911">
			<property name="Sound_start" value="pistol_s_fire"/>
			<property name="BarrelScale" value="0.8,0.8,0.8"/>
		</item_property_overrides>
		
		<item_property_overrides name="HD Colt M1911A">
			<property name="Sound_start" value="pistol_s_fire"/>
			<property name="BarrelScale" value="0.8,0.8,0.8"/>
		</item_property_overrides>

			<effect_group tiered="false">
			<passive_effect name="NoiseMultiplier" operation="perc_subtract" value="100" tags="primary,secondary"/>
			<display_value name="dNoiseMultiplier" value="25"/>
				<passive_effect name="BlockDamage" operation="base_subtract" value="5"/>
				<passive_effect name="SpreadMultiplierHip" operation="perc_add" value=".1"/>
				<passive_effect name="DamageFalloffRange" operation="perc_add" value="-.10"/>
				<passive_effect name="IncrementalSpreadMultiplier" operation="perc_add" value=".10"/>
				
				<triggered_effect trigger="onSelfHoldingItemCreated" action="AttachPrefabToHeldItem" prefab="#@modfolder:Resources/HDColtM1911Silencer.unity3d?HDColtM1911Silencer.Prefab" parent_transform="Attachments/Barrel"/>
			</effect_group>
		<property name="PickupJournalEntry" value="augmentGunsTip"/>
			<property name="Group" value="AllMods,ModAttachment"/>
			<property name="EconomicValue" value="1000"/>
			<property name="EconomicBundleSize" value="1"/>
			<property name="SellableToTrader" value="true"/>
	</item_modifier>
	
